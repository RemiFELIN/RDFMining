prefix spqr: <http://ns.inria.fr/sparql-extension/spqr/>
select 

(xt:jour(now()) as ?j)
(xt:day("2001-01-01"^^xsd:date) as ?d)
(spqr:romain(1959) as ?s)
(spqr:digit("CIX") as ?c)

where  {}