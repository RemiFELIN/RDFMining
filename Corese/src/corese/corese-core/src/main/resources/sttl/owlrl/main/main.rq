template st:main {
    st:call-template-with(st:pperror, st:display, ?map)
}
where {
    bind (st:visit(st:start, st:trace) as ?vis)
    bind (st:call-template(st:start)   as ?b)
    bind (st:errormap()                as ?map)
}  

function st:process(?x) {
    ?x
}



