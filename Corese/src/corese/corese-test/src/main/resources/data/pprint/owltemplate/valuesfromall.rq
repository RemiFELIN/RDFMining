template {
  if (bound(?t1) || bound(?t2), "DataAllValuesFrom", "ObjectAllValuesFrom")  
  "(" kg:uri(?p) " " kg:uri(?z) ")"  
}
where {
  ?in owl:allValuesFrom ?z;
  owl:onProperty ?p .
    optional { ?z a ?t1 filter(?t1 = rdfs:Datatype) }
    optional { ?p a ?t2 filter(?t2 = owl:DatatypeProperty) }
}

pragma { kg:template kg:priority 1 }
